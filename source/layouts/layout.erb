      })(document);
    </script>
</head>

<body>
<div ng-app="fcastsApp">
        <div ng-controller="FcastsCtrl">


            <%= partial "header" %>

            <%= yield %>

            <%= partial "footer" %>

          <%= javascript_include_tag  "application", :defer => "defer" %>

      </div>
  </div>

</body>
